{
  "name": "nano-style",
  "version": "2.0.0-0",
  "description": "Functional css-in-js for react",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "start": "x0 dev docs/App.js -op 9000",
    "build": "x0 build docs/App.js -d docs --static",
    "size": "npm run prepublish && bundlesize",
    "cover": "nyc report --reporter=html --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc ava"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {},
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "2 kb"
    }
  ]
}
