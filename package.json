{
  "name": "nano-style",
  "version": "2.0.0-0",
  "description": "Functional css-in-js for react",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "start": "x0 docs -op 9000",
    "build": "x0 build docs",
    "size": "npm run prepublish && bundlesize",
    "cover": "nyc report --reporter=html --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc ava"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@compositor/x0": "^6.0.0-4",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundlesize": "^0.17.0",
    "nyc": "^12.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "2 kb"
    }
  ]
}
